{
  "name": "@foxpage/foxpage-entry-react",
  "version": "0.5.3",
  "description": "foxpage csr react entry",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "start": "npm run build:dev -- --watch",
    "build": "npm run build:es & npm run build:dist",
    "build:dev": "cross-env NODE_ENV=development webpack --config config/webpack.config.js",
    "build:dist": "cross-env NODE_ENV=production webpack --config config/webpack.config.js",
    "build:es": "tsc --project tsconfig.prod.json",
    "build:npm": "npm run build -- --skipLibCheck",
    "clean": "rimraf lib & rimraf dist",
    "prepublishOnly": "npm install && npm run build:npm",
    "babel": "babel src/ --out-dir lib --extensions .js,.ts,.tsx",
    "analyze": "ANALYZE=1 webpack",
    "lint:type": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "test:coverage": "npm run test -- --coverage"
  },
  "keywords": [
    "foxpage-entry-react",
    "csr",
    "entry"
  ],
  "author": "drxiao",
  "license": "MIT",
  "files": [
    "lib/**",
    "dist/**"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/preset-env": "^7.7.5",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@foxpage/foxpage-types": "^0.5.2",
    "@types/react": "^16.9.1",
    "@types/react-dom": "^16.8.5",
    "@types/react-helmet": "^6.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.13.0",
    "cross-env": "^5.1.1",
    "css-loader": "^3.2.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "rimraf": "^3.0.0",
    "style-loader": "^1.0.0",
    "typescript": "^4.0.2",
    "webpack": "^4.41.0",
    "webpack-bundle-analyzer": "^3.5.2",
    "webpack-cli": "^3.3.9"
  },
  "dependencies": {
    "@foxpage/foxpage-browser-loader": "^0.3.3",
    "antd": "^4.1.0",
    "core-js": "^3.6.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0"
  }
}
